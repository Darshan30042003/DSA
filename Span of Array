SPAN=MAX-MIN;


package problems;
import java.util.Scanner;
import java.util.Arrays;
public class trailing_zero {
	public static void main(String[] args) {
		int[] arr= {10,20,30,40,50};
		int max=arr[0];
		for(int i=0;i<arr.length;i++) {
			if(arr[i]>max) {
				max=arr[i];
			}
		}
		int min=arr[0];
		for(int i=0;i<arr.length;i++) {
			if(arr[i]<min) {
				min=arr[i];
			}
		}
		int span=max-min;
		System.out.println(span);
		
		
		
	}
}

O/P=40
